{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2020"
        ],
        "lib": ["DOM", "DOM.Iterable", "ES2020"],
        "rootDir": "ts",
        "outDir": "js",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "noEmitOnError": true,
        "allowImportingTsExtensions": false
    },
    "include": [
        "ts/**/*",
        "types/**/*.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
    "include": ["ts/**/*"],
    "exclude": ["node_modules"]
}
